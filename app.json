{
  "expo": {
    "name": "Aplicativo IPTV",
    "slug": "aplicativo-iptv",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1C1C1E"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.iptvapp.aplicativo"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1C1C1E"
      },
      "package": "com.iptvapp.aplicativo"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-av"
    ],
    "sdkVersion": "53.0.0"
  }
} 